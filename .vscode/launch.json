    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: abstralg rs [encode]",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/poetry",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run",
                "abstralg",
                "rs",
                "I Love Abstract Algebra"
            ]
        },
        {
            "name": "Python: abstralg rs [decode]",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/poetry",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run",
                "abstralg",
                "rs",
                "-d",
                ""
            ]
        }
    ]
}