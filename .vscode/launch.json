{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: codechain encode",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/poetry",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run",
                "codechain",
                "encode",
                "-f",
                "./tests/data/test_pipeline.yaml",
                "I Love Abstract Algebra"
            ]
        },
        {
            "name": "Python: codechain decode",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/poetry",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run",
                "codechain",
                "decode",
                "-f",
                "./tests/data/test_pipeline.yaml",
                ""
            ]
        },
    ]
}